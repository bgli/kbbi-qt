qmake: FORCE
	@$(QMAKE) -o Makefile KBBI-Qt.pro

qmake_all: FORCE


all: Makefile $(TARGET)

dist: distdir FORCE
	(cd `dirname $(DISTDIR)` && $(TAR) $(DISTNAME).tar $(DISTNAME) && $(COMPRESS) $(DISTNAME).tar) && $(MOVE) `dirname $(DISTDIR)`/$(DISTNAME).tar.gz . && $(DEL_FILE) -r $(DISTDIR)

distdir: FORCE
	@test -d $(DISTDIR) || mkdir -p $(DISTDIR)
	$(COPY_FILE) --parents $(DIST) $(DISTDIR)/
	$(COPY_FILE) --parents resource.qrc $(DISTDIR)/
	$(COPY_FILE) --parents mainwindow.h tentang.h $(DISTDIR)/
	$(COPY_FILE) --parents main.cpp mainwindow.cpp tentang.cpp $(DISTDIR)/
	$(COPY_FILE) --parents mainwindow.ui tentang.ui $(DISTDIR)/


clean: compiler_clean 
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


distclean: clean 
	-$(DEL_FILE) $(TARGET) 
	-$(DEL_FILE) Makefile


####### Sub-libraries

mocclean: compiler_moc_header_clean compiler_moc_source_clean

mocables: compiler_moc_header_make_all compiler_moc_source_make_all

check: first

benchmark: first

compiler_rcc_make_all: qrc_resource.cpp
compiler_rcc_clean:
	-$(DEL_FILE) qrc_resource.cpp
qrc_resource.cpp: resource.qrc \
		kbbi-qt.png \
		data/KBBI.db
	/usr/lib/qt5/bin/rcc -name resource resource.qrc -o qrc_resource.cpp

compiler_moc_header_make_all: moc_mainwindow.cpp moc_tentang.cpp
compiler_moc_header_clean:
	-$(DEL_FILE) moc_mainwindow.cpp moc_tentang.cpp
moc_mainwindow.cpp: mainwindow.h
	/usr/lib/qt5/bin/moc $(DEFINES) -I/usr/lib/qt5/mkspecs/linux-g++ -I./src -I/usr/include/qt5 -I/usr/include/qt5/QtWidgets -I/usr/include/qt5/QtGui -I/usr/include/qt5/QtSql -I/usr/include/qt5/QtCore -I/usr/include/c++/4.9.4 -I/usr/include/c++/4.9.4/x86_64-unknown-linux-gnu -I/usr/include/c++/4.9.4/backward -I/usr/lib/gcc/x86_64-unknown-linux-gnu/4.9.4/include -I/usr/local/include -I/usr/lib/gcc/x86_64-unknown-linux-gnu/4.9.4/include-fixed -I/usr/include mainwindow.h -o moc_mainwindow.cpp

moc_tentang.cpp: tentang.h
	/usr/lib/qt5/bin/moc $(DEFINES) -I/usr/lib/qt5/mkspecs/linux-g++ -I./src -I/usr/include/qt5 -I/usr/include/qt5/QtWidgets -I/usr/include/qt5/QtGui -I/usr/include/qt5/QtSql -I/usr/include/qt5/QtCore -I/usr/include/c++/4.9.4 -I/usr/include/c++/4.9.4/x86_64-unknown-linux-gnu -I/usr/include/c++/4.9.4/backward -I/usr/lib/gcc/x86_64-unknown-linux-gnu/4.9.4/include -I/usr/local/include -I/usr/lib/gcc/x86_64-unknown-linux-gnu/4.9.4/include-fixed -I/usr/include tentang.h -o moc_tentang.cpp

compiler_moc_source_make_all:
compiler_moc_source_clean:
compiler_uic_make_all: ui_mainwindow.h ui_tentang.h
compiler_uic_clean:
	-$(DEL_FILE) ui_mainwindow.h ui_tentang.h
ui_mainwindow.h: mainwindow.ui
	/usr/lib/qt5/bin/uic mainwindow.ui -o ui_mainwindow.h

ui_tentang.h: tentang.ui
	/usr/lib/qt5/bin/uic tentang.ui -o ui_tentang.h

compiler_yacc_decl_make_all:
compiler_yacc_decl_clean:
compiler_yacc_impl_make_all:
compiler_yacc_impl_clean:
compiler_lex_make_all:
compiler_lex_clean:
compiler_clean: compiler_rcc_clean compiler_moc_header_clean compiler_uic_clean 

####### Compile

main.o: main.cpp mainwindow.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o main.cpp

mainwindow.o: mainwindow.cpp mainwindow.h \
		ui_mainwindow.h \
		tentang.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o mainwindow.o mainwindow.cpp

tentang.o: tentang.cpp tentang.h \
		ui_tentang.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o tentang.o tentang.cpp

qrc_resource.o: qrc_resource.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o qrc_resource.o qrc_resource.cpp

moc_mainwindow.o: moc_mainwindow.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_mainwindow.o moc_mainwindow.cpp

moc_tentang.o: moc_tentang.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_tentang.o moc_tentang.cpp

####### Preparation before install

preparation:
	echo "Preparing .."
	mkdir icons
	for i in 256 128 64 48 32 24 16; do \
			mkdir -p icons/$$i; \
			convert kbbi-qt.png -resize $$ix$$i icons/$$i/kbbi-qt.png; \
	done
	echo '[Desktop Entry]\nVersion=0.1\nName=KBBI Qt\nComment=Aplikasi Kamus Besar Bahasa Indonesia\nExec=/usr/bin/KBBI-Qt\nIcon=kbbi-qt\nTerminal=false\nType=Application\nCategories=Utility;' > kbbi-qt.desktop

####### Install

install: preparation  FORCE
	echo "Installing .."
	$(INSTALL_PROGRAM) $(QMAKE_TARGET) $(BINDIR)$(TARGET)
	$(INSTALL_FILE) data/KBBI.db /opt/
	mkdir -p icons
	for i in 256 128 64 48 32 24 16; do \
			$(INSTALL_FILE) icons/$$i/kbbi-qt.png \
			/usr/share/icons/hicolor/$${i}x$${i}/kbbi-qt.png; \
	done
	$(INSTALL_FILE) kbbi-qt.desktop /usr/share/applications/

uninstall:  FORCE
	rm -f /usr/share/applications/kbbi-qt.desktop
	rm -f /opt/KBBI.db
	rm -f $(BINDIR)$(TARGET)
	for i in 256 128 64 48 32 24 16; do \
		rm -f /usr/share/icons/hicolor/$${i}x$${i}/kbbi-qt.png; \
	done
FORCE:

